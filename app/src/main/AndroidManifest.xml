<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.correios.edwinos.consultacorreios" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.correios.edwinos.consultacorreios.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.correios.edwinos.consultacorreios.InsertActivity"
            android:label="@string/title_activity_insert"
            android:theme="@android:style/Theme.Dialog"
        >
            <intent-filter>
                <action android:name="com.correios.edwinos.consultacorreios.InsertActivity"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>

    </application>

</manifest>
